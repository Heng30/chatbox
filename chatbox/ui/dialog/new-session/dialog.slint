import { CDialog } from "../../base/cdialog.slint";
import { Store } from "../../store.slint";

export component NewSessionDialog inherits CDialog {
    visible: Store.new-session-dialog-setting.is-dialog-show;
    background: rect.background;
    title: "会话配置";

    rect := Rectangle {
        background: gray;
    }

    ok-clicked => {
        Store.new-session-dialog-setting.is-dialog-show = false;
    }

    cancel-clicked => {
        Store.new-session-dialog-setting.is-dialog-show = false;
    }
}
